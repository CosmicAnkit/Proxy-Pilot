{
    "manifest_version": 2,
    "name": "Proxy Pilot",
    "version": "1.0",
    "description": "Your Ulitmate Proxy Guide",
    "permissions": ["proxy", "storage"],
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/icon16.png",
            "48": "icons/icon48.png",
            "128": "icons/icon128.png"
        }
    },
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    }
}
